{
  "name": "upt-www",
  "version": "0.0.4",
  "description": "",
  "main": "index.js",
  "scripts": {
    "live": "nodemon index.js --watch ./core/",
    "mongo": "mongod --dbpath ./mongo/ --bind_ip 127.0.0.1",
    "start": "node ./index.js",
    "dump": "mongodump -d upt --excludeCollection sessions --excludeCollection exercise_verifiers --excludeCollection quiz_verifiers --excludeCollection users --excludeCollection solved_exercises --excludeCollection exercise_revisions",
    "backup": "mongodump -d upt -o backup",
    "restore": "mongorestore -d upt dump/upt",
    "clean": "rm -r dump",
    "pretty": "prettier --write --tab-width 2 \"core/**/*.js\"",
    "precommit": "lint-staged"
  },
  "author": "Kerdo",
  "license": "ISC",
  "lint-staged": {
    "*.js": [
      "npm run pretty",
      "git add"
    ]
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "ejs-lint": "^0.3.0",
    "express": "^4.17.1",
    "firebase": "^7.5.0",
    "firebase-admin": "^8.8.0",
    "lodash": "^4.17.15",
    "mathjax-node": "^2.1.1",
    "moment": "^2.24.0",
    "mongodb": "^3.3.5",
    "mongoose": "^5.9.12",
    "request": "^2.88.2",
    "showdown": "^1.9.1",
    "sitemap": "^2.1.0"
  },
  "devDependencies": {
    "husky": "^4.2.3",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1"
  }
}
